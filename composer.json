{
    "name": "newpaymentgateway/laravel",
    "type": "library",
    "description": "Official laravel plugin for cryptocurrency payment gateway",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.8.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paymentgateway\\PaymentGatewayServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
